<% include ./team_verification_dashboard_header.ejs %>



<div class="content-body">

    <div class="container-fluid mt-3">


<div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <div class="active-member">
                        <div class="table-responsive">
                            <table class="table table-xs mb-0" id="myTable">
                                <h3>Approved Professionals</h3>
                                <input type="text"  class="form-control" id="myInput" onkeyup="myFunction()" placeholder="Search By Mobile Number.." title="Type in a name">
                                          
                                <thead>
                                    <tr>
                                        
                                        <th>Number</th>
                                        <th>Name</th>
                                        <th>Category</th>
                                        <th>Address</th>
                                        <th>Pincode</th>
                                        <th>Image</th>
                                        <th>Recharge Done</th>
                                        <th>S.No.</th>
                                     
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(i=0;i<result.length;i++) { %>
                                    <tr>
                                       
                                            <td><%=result[i].number%></td>
                                        <td><a href="/team-verification-dashboard/single_approval_details?id=<%=result[i].id%>" style="font-size:15px"><%=result[i].name%></a></td>
                                        <td><%=result[i].categoryname%></td>
                                        <td>
                                            <span><%=result[i].local_address%></span>
                                        </td>
                                        <td><%=result[i].pincode%></td>
                                        
                                       <% if(result[i].recharge_value==null || result[i].recharge_value=="null" || result[i].recharge_value=='' || result[i].recharge_value==[] || result[i].recharge_value==0 || result[i].recharge_value=='0'){%>
                                        <td><span style="font-size:20px">&#10060;</span></td>
                                    <% } else { %>
                                        <td><span class="label gradient-4 rounded"><%=result[i].recharge_value%></span></td>
                                        <% } %>
                                       
                                      
                                        <td><%= i %></td>
                                        
                                        
                                    </tr>

                                    <% } %>

                                  
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>                        
        </div>
    </div>
    </div>
    </div>

      <!--**********************************
            Footer start
        ***********************************-->
        <div class="footer">
                <div class="copyright">
                    <p>Copyright &copy;2019 Delo Services Pvt. Ltd</p>
                </div>
            </div>
            <!--**********************************
                Footer end
            ***********************************-->
        </div>
        <!--**********************************
            Main wrapper end
        ***********************************-->
    
        <!--**********************************
            Scripts
        ***********************************-->
        <script src="/booking_dashboard/plugins/common/common.min.js"></script>
        <script src="/booking_dashboard/js/custom.min.js"></script>
        <script src="/booking_dashboard/js/settings.js"></script>
        <script src="/booking_dashboard/js/gleek.js"></script>
        <script src="/booking_dashboard/js/styleSwitcher.js"></script>
    
    

        <script>



                function myFunction() {
                    var input, filter, table, tr, td, i, txtValue;
                    input = document.getElementById("myInput");
                    filter = input.value.toUpperCase();
                    table = document.getElementById("myTable");
                    tr = table.getElementsByTagName("tr");
                    for (i = 0; i < tr.length; i++) {
                      td = tr[i].getElementsByTagName("td")[0];
                      if (td) {
                        txtValue = td.textContent || td.innerText;
                        if (txtValue.toUpperCase().indexOf(filter) > -1) {
                          tr[i].style.display = "";
                        } else {
                          tr[i].style.display = "none";
                        }
                      }
                    }
                  }
                </script>


    </body>
    
    </html>