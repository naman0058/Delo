<script>

$('.send-message').click(function(){
    if($('.message').val()==null || $('.message').val()=="null" || $('.message').val()=="" || $('.message').val()==[]  ){
        alert("Enter Message...")
    }
    else{
        const message = $('.message').val()
        var a = confirm("Are you sure to send this message...")
        if(a){
            $.post('/team_dashboard/send_message',{message},data=>{
                alert("Send Successfully...")
            })
           
        }
        else{
            alert("Sending Failed...")
        }
    }
})


