  <style>
    /* width */
    ::-webkit-scrollbar {
      width: 8px;
    }
    
    /* Track */
    ::-webkit-scrollbar-track {
      box-shadow: inset 0 0 5px grey; 
      border-radius: 6px;
    }
     
    /* Handle */
    ::-webkit-scrollbar-thumb {
      background:#1F5AB0; 
      border-radius: 6px;
    }
    
