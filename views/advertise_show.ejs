<% include ./delo_listing_dashboard_header.ejs %>
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">

            <div class="container-fluid mt-3">
               <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="active-member">
                                    <div class="table-responsive">
                                        <table class="table table-xs mb-0">
                                            <h3>Advertise Request</h3>
                                            <thead>
                                                <tr>
                                                    <th>S.No.</th>
                                                    <th>Name</th>
                                                    <th>Categoy Name</th>
                                                    <th>Address</th>
                                                    <th>Number</th>
                                                    <th>Image</th>
                                                 </tr>
                                            </thead>
                                            <tbody>
                                                <% for(i=0;i<result.length;i++){ %>
                                             <tr>
                                                <td><%=result[i].name%></td>
                                                <td><%=result[i].name%></td>
                                                    <td><%=result[i].categoryname%></td>
                                                <td><%=result[i].address1%></td>
                                                <td><%=result[i].number%></td>
                                                <td><%=result[i].number%></td>
                                                <td><button type="button" class="btn btn-primary" id="<%=result[i].id%>">Accept</button></td>
                                                <td><button type="button" class="btn btn-danger" id="<%=result[i].id%>">Delete</button></td>
                                             </tr>
                                             <% } %>
                                             
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>                        
                    </div>
                </div>

                
                </div>

                

              
                    
            </div>
            <!-- #/ container -->
        </div>
