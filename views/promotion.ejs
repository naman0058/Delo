<script>


$('.send').click(function(){
    if( $('#number').val() == [] || $('#number').val() == "" || $('#number').val() == null || $('#number').val() == "null" ){
        alert("Enter Mobile Number...")
    }
    else{

        const number = $('#number').val()
        const message = $('#message').val()
        $.post('/blogs/send',{number,message},data=>{
            alert('Send Successfully')
        })
    }
})







$('.updateapp').click(function(){
    if( $('#updatemessage').val() == [] || $('#updatemessage').val() == "" || $('#updatemessage').val() == null || $('#updatemessage').val() == "null" ){
        alert("Enter Message...")
    }
    else{

       const updatemessage = $('#updatemessage').val()
        $.post('/blogs/updatemessage',{updatemessage},data=>{
            alert('Send Successfully')
        })
    }
})


$('.send1').click(function(){
    if( $('#number1').val() == [] || $('#number1').val() == "" || $('#number1').val() == null || $('#number1').val() == "null" ){
        alert("Enter Mobile Number...")
    }
    else if( $('#message1').val() == [] || $('#message1').val() == "" || $('#message1').val() == null || $('#message1').val() == "null" ){
        alert("Enter Message...")
    }
    else{

        const number = $('#number1').val()
        const message = $('#message1').val()
        $.post('/blogs/send',{number,message},data=>{
            alert('Send Successfully')
        })
    }
})

