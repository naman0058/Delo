<!--  // Manage Camoign Modal -->

<div class="modal fade bd-example-modal-sm" id="review" role="dialog">
  <div class="modal-dialog modal-sm">
 
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header" style="background-color: black;">
