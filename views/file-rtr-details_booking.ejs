<div class="content-body">

    <div class="container-fluid mt-3">



            <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="active-member">
                                    <div class="table-responsive">
                                        <table class="table table-xs mb-0" id="myTable" >
                                            <h3>File RTR Booking</h3>
                                            <input type="text"  class="form-control" id="myInput" onkeyup="myFunction()" placeholder="Search By Cusotmer Name.." title="Type in a name">
                                            <thead>
                                                <tr>
                                                    <th>Number</th>
                                                    <th>Customers</th>
                                                    <th>Email</th>
                                                    <th>Address</th>
                                                    <th>Pincode</th>
                                                    <th>Booking Date and Time</th>
                                               
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for(i=0;i<result.length;i++) { %>
                                                <tr>
                                                        <td><%=result[i].number%></td>
                                                    <td><a href="/booking_dashboard/file_rtr_booking_details?id=<%=result[i].id%>" style="font-size:15px"><%=result[i].name%></a></td>
                                                    <td><%=result[i].email%></td>
                                                    <td>
                                                        <span><%=result[i].address1%></span>
                                                    </td>
                                                    <td><%=result[i].pincode%></td>
                                                    <td><%=result[i].booking_date%> at <%=result[i].booking_time%></td>
                                                  
                                                   
                                                  
                                                     
                                                   
                                                    
                                                   
                                                </tr>


                                                <% } %>

                                              
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>                        
                    </div>
                </div>
    </div>
    </div>

