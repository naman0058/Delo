        <td data-label="View Details"> <a href="/booking/information?id=<%=result[0][i].id%>"><button type="button" class="btn btn-primary" >View Details</button></a></td>

      </tr>
     	<% } %>
    
    </tbody>
  </table>
  <% } else { %>}
  <h1><a href="/"><button type="button" class="btn btn-info">Book Now</button></a></h1>
  <% } %>
