  <script>
  
      $('.checkout').hide()
      
      $(".myBtn").click(function(){
             booking_id = $(this).attr('id');
            price = $(this).attr('price');
            subcategoryid = $(this).attr('subcategoryid')
            const categoryid = $(this).attr('categoryid');
             
             $.post('/booking/cart',{booking_id,price,subcategoryid,categoryid},data=>{
                if(data =='Success'){

        $('.checkout').show()
              }
              else if(data == 'Can not book two different categories services simultaneously. Replace cart ?'){
                const a = confirm('Can not book two different categories services simultaneously. Replace cart ?');
                if(a){
                    $.post('/booking/cart/replace',{booking_id,price,subcategoryid,categoryid},data=>{
                    if(data =='Success'){
                     
      $('.checkout').show()
      }
                  })
                }
                 
              }
              else if(data == 'Beacuse you are a team member of this services, you can not book this service'){
                alert('Beacuse you are a team member of this category, you can not book this service')
              }
              else{
                window.location.href = '/login'
              }
            })
      
                 
                 
      
      
      })
      
      
      
      $.getJSON('/booking/cart/all',data=>{
        if(data[0]){
         $('.checkout').show()
        }
        
      })
      
      
      </script>
      
     
     
